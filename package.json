{
  "name": "telegram-bot-vantage",
  "version": "1.0.0",
  "description": "telegram bot with admin panel for screenshot submissions",
  "main": "dist/server.js",
  "scripts": {
    "build": "npm run build:ts && npm run build:functions || true && npm run copy:assets",
    "build:ts": "tsc",
    "build:functions": "tsc --project tsconfig.functions.json",
    "copy:assets": "mkdir -p dist/public && cp -r public/* dist/public/ 2>/dev/null || true && cp tgbot.mp4 dist/ 2>/dev/null || true",
    "start": "node dist/server.js",
    "postinstall": "npm run build",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "type-check": "tsc --noEmit",
    "setup-webhook": "node dist/scripts/setup-webhook.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "admin",
    "supabase"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "node-telegram-bot-api": "^0.64.0",
    "@supabase/supabase-js": "^2.39.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@netlify/functions": "^2.4.0",
    "dotenv": "^16.3.1",
    "express-session": "^1.17.3",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/node-telegram-bot-api": "^0.64.0",
    "@types/express-session": "^1.17.7",
    "@types/uuid": "^9.0.1",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "@types/bcryptjs": "^2.4.2"
  }
}
